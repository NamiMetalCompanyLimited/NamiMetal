{
  "ConnectionStrings": {
    "Default": "Server=w05.onedata.vn,2019\\SQL2019;Database=namispac_store_dev;User Id=namispac_admin;Password=Nami@$#2022"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Error",
        "Microsoft.AspNetCore.Mvc.Infrastructure.ObjectResultExecutor": "Error",
        "Microsoft.EntityFrameworkCore.Infrastructure": "Error",
        "Microsoft.EntityFrameworkCore.Database.Command": "Error",
        "DotNetCore.CAP": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext",
      {
        "Name": "WithCorrelationIdHeader",
        "Args": {
          "headerKey": "x-request-id"
        }
      }
    ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/.log",
          "rollingInterval": "Hour",
          "retainedFileCountLimit": 360,
          "encoding": "System.Text.Encoding::UTF8"
        }
      }
    ]
  }
}
