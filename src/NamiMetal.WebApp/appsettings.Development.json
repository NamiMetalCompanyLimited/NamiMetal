{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Debug",
        "Microsoft.AspNetCore.Mvc.Infrastructure.ObjectResultExecutor": "Debug",
        "Microsoft.EntityFrameworkCore.Infrastructure": "Debug",
        "Microsoft.EntityFrameworkCore.Database.Command": "Debug",
        "DotNetCore.CAP": "Debug"
      }
    },
    "Enrich": [
      "FromLogContext"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/.log",
          "rollingInterval": "Hour",
          "retainedFileCountLimit": 360,
          "encoding": "System.Text.Encoding::UTF8"
        }
      }
    ]
  },
  "RemoteServices": {
    "Default": {
      "BaseUrl": "https://localhost:44390/"
    }
  }
}
